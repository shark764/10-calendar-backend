{
  "name": "calendar-backend",
  "version": "1.0.0",
  "description": "Backend project written in Node.js",
  "author": "Farid Hernandez <farid.hdz.64@gmail.com>",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc",
    "start": "node ./bin/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q bin/index.js\"",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "typescript": "tsc --noEmit",
    "prettify": "yarn run format:write && yarn run lint:fix",
    "full-check": "yarn run prettify && yarn run typescript",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "_moduleAliases": {
    "@": "bin",
    "@modules": "bin/modules",
    "@services": "bin/services",
    "@controllers": "bin/controllers",
    "@repository": "bin/repository",
    "@routers": "bin/routers",
    "@routes": "bin/routes"
  },
  "dependencies": {
    "@2600hz/commio-native-utilities": "^0.5.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "module-alias": "^2.2.2",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.0",
    "lint-staged": "^13.0.2",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.1"
  },
  "license": "MIT"
}
